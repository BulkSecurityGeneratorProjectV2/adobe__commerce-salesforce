        <header
            jcr:primaryType="nt:unstructured"
            sling:resourceType="commerce/demandware/components/placeholder/header"/>
        <par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="commerce/demandware/components/placeholder/parsys">
            <title
                jcr:primaryType="nt:unstructured"
