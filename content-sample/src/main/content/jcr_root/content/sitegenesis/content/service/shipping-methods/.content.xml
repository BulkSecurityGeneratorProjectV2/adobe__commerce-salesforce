<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/apps/commerce/demandware/templates/page-content"
        jcr:mixinTypes="[mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Shipping Methods"
        dwreOnline="true"
        dwreSearchable="true"
        dwreFolder="[shippinginfo]"
        sling:resourceType="commerce/demandware/components/page/page">
        <header
            jcr:primaryType="nt:unstructured"
            sling:resourceType="commerce/demandware/components/placeholder/header"/>
        <par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="commerce/demandware/components/placeholder/parsys">
            <title
                jcr:primaryType="nt:unstructured"
                sling:resourceType="commerce/demandware/components/content/title"/>
            <text
                jcr:primaryType="nt:unstructured"
                sling:resourceType="commerce/demandware/components/content/text"
                text="&lt;p>&lt;i>SiteGenesis does not ship to AK, HI, or P.O. Boxes.&lt;/i>&lt;/p>&#xa;&lt;table>&#xa;&lt;tbody>&lt;tr>&lt;th>Shipment Cost&lt;/th>&#xa;&lt;th>UPS Ground&lt;/th>&#xa;&lt;th>2nd Day Air&lt;/th>&#xa;&lt;th>Next Day Air&lt;/th>&#xa;&lt;/tr>&lt;tr>&lt;td>$0-$99.99&lt;/td>&#xa;&lt;td>$5.99&lt;/td>&#xa;&lt;td>$9.99&lt;/td>&#xa;&lt;td>$15.99&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td>$100.00 - $199.99&lt;/td>&#xa;&lt;td>$7.99&lt;/td>&#xa;&lt;td>$11.99&lt;/td>&#xa;&lt;td>$19.99&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td>$200.00 - $499.9&lt;/td>&#xa;&lt;td>$9.99&lt;/td>&#xa;&lt;td>$15.99&lt;/td>&#xa;&lt;td>$21.99&lt;/td>&#xa;&lt;/tr>&lt;tr>&lt;td>$500 +&lt;/td>&#xa;&lt;td>$15.99&lt;/td>&#xa;&lt;td>$20.99&lt;/td>&#xa;&lt;td>$29.99&lt;/td>&#xa;&lt;/tr>&lt;/tbody>&lt;/table>&#xa;&lt;p>&lt;b>Note:&amp;nbsp;&lt;/b>All Televisions must ship using ground shipping, 2nd Day Air and Next Day Air are not available shipping options for televisions.&lt;/p>&#xa;"
                textIsRich="true"/>
        </par>
        <footer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="commerce/demandware/components/placeholder/footer"/>
    </jcr:content>
</jcr:root>
