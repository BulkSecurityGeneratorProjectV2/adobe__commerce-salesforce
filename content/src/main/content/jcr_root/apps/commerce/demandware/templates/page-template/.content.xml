<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Use this template to create Velocity bases rendering template pages."
    jcr:primaryType="cq:Template"
    jcr:title="Demandware Template"
    allowedPaths="[/content/sitegenesis.*/templates/?]"
    ranking="{Long}200">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="commerce/demandware/components/page/page_template">
        <header
            jcr:primaryType="nt:unstructured"
            sling:resourceType="commerce/demandware/components/placeholder/header"/>
        <par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="commerce/demandware/components/placeholder/parsys">
            <text
                jcr:primaryType="nt:unstructured"
                sling:resourceType="commerce/demandware/components/content/text"
                text="&lt;p>This is a content page&lt;/p>"
                textIsRich="true"/>
        </par>
        <footer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="commerce/demandware/components/placeholder/footer"/>
    </jcr:content>
</jcr:root>
